{
  "name": "dairy-track-web",
  "version": "1.0.0",
  "description": "Dairy farm management web application",
  "private": true,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "apexcharts": "^4.7.0",
    "bootstrap": "^5.3.6",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.11.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.509.0",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-bootstrap": "^2.10.9",
    "react-chartjs-2": "^5.3.0",
    "react-currency-input-field": "^3.10.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^6.0.3",
    "react-number-format": "^5.4.4",
    "react-phone-input-2": "^2.15.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.6.13",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "concurrently \"npm run frontend\" \"npm run api:all\"",
    "frontend": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "api:all": "concurrently \"npm run api:feed\" \"npm run api:milk\" \"npm run api:health\" \"npm run api:selling\"",
    "api:feed": "cd ../dairyTrack-api/FeedConsumption && npm start 0.0.0:5003",
    "api:milk": "cd ../dairyTrack-api/MilkProduction && python run.py 0.0.0:5000",
    "api:health": "cd ../dairyTrack-api/HealthCheck && python manage.py runserver 0.0.0.0:5002",
    "api:selling": "cd ../dairyTrack-api/Selling && python manage.py runserver 0.0.0.0:5001",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
